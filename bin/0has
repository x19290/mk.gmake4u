#!/usr/bin/env python3


def main():
    r'''
    call ./~.has.__main__.main()
    if this command is called ./bin/0has
    '''
    from importlib import import_module
    from pathlib import Path
    from sys import path as pythonpath
    __path__ = Path(__file__)
    stem = __path__.stem[1:]
    __path__ = __path__.resolve().parent

    def pkg():
        for y in __path__.iterdir():
            y = y.name
            if y[:1].isalpha():
                return y  # use it as the pkgname

    pythonpath[:0] = __path__.parent.__str__(),
    mod = r'x19290.%s.%s.__main__' % (pkg(), stem)
    __main__ = import_module(mod)
    __main__.main()


if __name__ == r'__main__':
    main()
